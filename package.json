{
  "name": "image-to-text",
  "version": "1.0.0",
  "scripts": {
    "build": "next build",
    "d": "yarn develop",
    "dev": "yarn develop",
    "develop": "yarn next:develop",
    "deploy": "yarn && yarn build && yarn export",
    "export": "next export",
    "format-check": "yarn prettier --list-different",
    "prettier": "prettier \"**/*.{css,js,json,jsx,md,mdx,scss,ts,tsx}\"",
    "lint": "yarn linter",
    "lint-fix": "yarn linter --fix",
    "linter": "eslint . --color",
    "serve": "next start -p 4000",
    "start": "yarn develop",
    "type-check": "tsc",
    "validate": "yarn format-check && yarn lint",
    "validate-fix": "yarn format && yarn lint-fix && yarn validate",
    "upgrade": "npm-check --update",
    "next:develop": "next dev"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "next": "^10.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.3.1",
    "styled-components": "^5.2.1",
    "tesseract.js": "^2.1.4",
    "webpack": "^5.24.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-flowtype": "^5.1.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-check": "^5.9.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.5"
  }
}
